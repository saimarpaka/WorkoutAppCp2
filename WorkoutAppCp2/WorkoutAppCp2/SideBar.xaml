    <pages:SideBarMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
                <local:WorkoutsList/>
            </x:Arguments>
    </NavigationPage>
      
  </MasterDetailPage.Detail>
</MasterDetailPage>